{{#form.element
  classNames='days'
  label=(t 'create.options.dates.label')
  property='options'
  as |el|
}}
  {{bootstrap-datepicker-inline
    id=el.id
    value=optionsBootstrapDatepicker
    multidate=true
    calendarWeeks=true
    todayHighlight=true
    language=i18n.locale
  }}
{{/form.element}}
